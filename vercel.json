{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "server": {
      "cors": {
        "origin": "https://communication-bb.vercel.app",
        "methods": ["GET", "POST", "PUT", "DELETE"],
        "credentials": true
      }
    }
  }
